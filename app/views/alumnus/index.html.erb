<p id="notice"><%= notice %></p>

<h1>Alumnus</h1>

<table>
  <thead>
    <tr>
      <th>Nama</th>
      <th>Kelas</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alumnus.each do |alumnu| %>
      <tr>
        <td><%= alumnu.nama %></td>
        <td><%= alumnu.kelas %></td>
        <td><%= link_to 'Show', alumnu %></td>
        <td><%= link_to 'Edit', edit_alumnu_path(alumnu) %></td>
        <td><%= link_to 'Destroy', alumnu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alumnu', new_alumnu_path %>

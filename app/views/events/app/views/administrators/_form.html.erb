<%= form_for @administrator do |f| %>
  <%= f.label(:ketua1, "Ketua 1 : ") %>
  <%= f.text_field(:ketua1) %> <br>

  <%= f.label(:fotoketua1, "Foto Ketua 1 : ") %>
  <%= f.file_field(:fotoketua1) %> <br>

  <%= f.label(:ketua2, "Ketua 2 : ") %>
  <%= f.text_field(:ketua2) %> <br>

  <%= f.label(:fotoketua2, "Foto Ketua 2 : ") %>
  <%= f.file_field(:fotoketua2) %> <br>

  <%= f.label(:bendahara1, "Bendahara 1 : ") %>
  <%= f.text_field(:bendahara1) %> <br>

  <%= f.label(:fotobendahara1, "Foto Bendahara 1 : ") %>
  <%= f.file_field(:fotobendahara1) %> <br>

  <%= f.label(:bendahara2, "Bendahara 2 : ") %>
  <%= f.text_field(:bendahara2) %> <br>

  <%= f.label(:fotobendahara2, "Foto Bendahara 2 : ") %>
  <%= f.file_field(:fotobendahara2) %> <br>

  <%= f.label(:pembina, "Pembina : ") %>
  <%= f.text_field(:pembina) %> <br>

  <%= f.label(:fotopembina, "Foto Pembina : ") %>
  <%= f.file_field(:fotopembina) %> <br>

  <%= f.label(:penasihat, "Penasihat : ") %>
  <%= f.text_field(:penasihat) %> <br>

  <%= f.label(:fotopenasihat, "Foto Penasihat : ") %>
  <%= f.file_field(:fotopenasihat) %> <br>

  <%= f.submit("Submit Administrator") %>
<% end %>